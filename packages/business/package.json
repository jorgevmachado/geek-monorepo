  },
  "dependencies": {
    "@geek/services": "*"
  },
  "devDependencies": {
    "@geek/typescript-config": "*",
    "@types/node": "^22.7.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "latest",
    "@turbo/gen": "^1.12.4"
  }
}
