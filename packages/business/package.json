{
  "name": "@geek/business",
  "version": "0.0.0",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
